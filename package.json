{
  "name": "bravo-react-typescript-boilerplate",
  "private": true,
  "version": "1.1.2",
  "type": "module",
  "scripts": {
    "local": "rimraf dev-dist && vite --force --mode local",
    "dev": "rimraf dev-dist && vite --force --mode dev",
    "sit": "rimraf dev-dist && vite --force --mode sit",
    "uat": "rimraf dev-dist && vite --force --mode uat",
    "build": "rimraf build && tsc && vite build",
    "build:dev": "rimraf build && tsc && vite build --mode dev",
    "build:sit": "rimraf build && tsc && vite build --mode sit",
    "build:uat": "rimraf build && tsc && vite build --mode uat",
    "build:production": "rimraf build && tsc && vite build --mode production",
    "checkTs": "tsc --noEmit",
    "prepare": "husky install",
    "preview": "vite preview",
    "lint": "npx eslint@8.57.0 src",
    "lint:fix": "yarn run lint --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn run prettier --write",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@bfi-finance/frontend-ui": "^3.0.57",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.7.0",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.13.7",
    "@mui/lab": "^5.0.0-alpha.135",
    "@mui/material": "^5.13.7",
    "@mui/utils": "^5.13.7",
    "@testing-library/dom": ">=7.21.4",
    "classnames": "^2.3.2",
    "date-fns": "^3.3.1",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-date-range": "^2.0.0-alpha.4",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.1",
    "react-router": "^6.4.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.3.0",
    "react-spring-bottom-sheet": "^3.4.1",
    "react-svg": "^15.1.10",
    "regenerator-runtime": "^0.13.9",
    "tss-react": "^4.8.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/loadable__component": "^5.13.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.2",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vitest": "^0.3.22",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^1.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn format"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.0.2"
}
