{
  "name": "bravo-react-typescript-boilerplate",
  "private": true,
  "version": "1.1.2",
  "type": "module",
  "scripts": {
    "local": "rimraf dev-dist && vite --force --mode local",
    "dev": "rimraf dev-dist && vite --force --mode dev",
    "sit": "rimraf dev-dist && vite --force --mode sit",
    "uat": "rimraf dev-dist && vite --force --mode uat",
    "build": "rimraf build && tsc && vite build",
    "build:dev": "rimraf build && tsc && vite build --mode dev",
    "build:sit": "rimraf build && tsc && vite build --mode sit",
    "build:uat": "rimraf build && tsc && vite build --mode uat",
    "build:production": "rimraf build && tsc && vite build --mode production",
    "checkTs": "tsc --noEmit",
    "prepare": "husky install",
    "preview": "vite preview",
    "lint": "npx eslint src test",
    "lint:fix": "yarn run lint --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn run prettier --write",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
